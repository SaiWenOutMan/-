}

.footer {
  width: 90%;
  height: 380rpx;
}
.footer .footer-item {
  width: 150rpx;
  height: 80rpx;
  border-radius: 6rpx;
  background: #f7f1f1;
  text-align: center;
  font-size: 30rpx;
  margin-right:15rpx;
  margin-bottom: 20rpx;
  line-height: 40px;
  float: left;
  color:#000000;
}