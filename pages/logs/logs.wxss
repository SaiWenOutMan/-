.header {
  width: 100%;
  height: 300rpx;
  background: url(https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1537086156&di=b86f1552bc677fa502aa0e12e8f94ab1&imgtype=jpg&er=1&src=http%3A%2F%2Fimg.redocn.com%2Fsheji%2F20180808%2Fwuhanlvyouguanggaohaibao_9606981.jpg) no-repeat;
  background-size: 100%;
  margin-bottom: 45rpx;
}
.content {
  width: 90%;
  margin-bottom: 30rpx;
}
.content .item {
  width: 220rpx;
  height: 200rpx;
  background: #f66143;
  text-align: center;
  line-height: 100px;
  border-radius: 20rpx 0 0 0;
  margin-right:3rpx;
  margin-bottom: 5rpx;
  float: left;
  color:#fff;
}
.content .item-jipiao {
  border-radius: 0 0 0 0;
  background: #3aa8f7;
  margin-left:3rpx;
}
.content .item-qiangpiao {
  border-radius: 0 20rpx 0 0;
  background: #35b9ff;
  margin-left:1.5rpx
}
.content .jingdian {
  border-radius: 0 0 0 0;
  background: #4ada83;
  margin-right:3rpx
}
.content .chuanpiao {
  border-radius: 0 0 0 0;
  background: #2cd0b5;
  margin-left:3rpx;
}
.content .zhuanche {
  border-radius: 0 0 0 0;
  background: #27cce2;
  margin-left:1.5rpx
}
.content .gentuan {
  width: 220rpx;
  height: 100rpx;
  background: #fa9f36;
  text-align: center;
  line-height: 50px;
  margin-right:3rpx;
  margin-bottom: 5rpx;
  float: left;
  color:#fff;
}
.content .ziyouxing {
